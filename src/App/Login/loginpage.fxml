<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="477.0" prefWidth="447.0" styleClass="AnchorPlaneBg" stylesheets="@login.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Login.LoginController">
    <children>
        <JFXButton fx:id="loginbutton" buttonType="RAISED" layoutX="165.0" layoutY="354.0" onAction="#Login" prefHeight="44.0" prefWidth="117.0" stylesheets="@login.css" text="Login" textFill="WHITE">
         <font>
            <Font name="Gill Sans MT" size="16.0" />
         </font>
         <ripplerFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#dfeece" />
                  <Stop color="#dfeece" offset="0.0074487895716946" />
                  <Stop color="#7aab6d" offset="1.0" />
               </stops>
            </LinearGradient>
         </ripplerFill>
         <styleClass>
            <String fx:value="loginBtn" />
            <String fx:value="login-Btn" />
         </styleClass></JFXButton>
        <JFXPasswordField fx:id="password" layoutX="126.0" layoutY="222.0" prefHeight="34.0" prefWidth="196.0" promptText="Password" styleClass="text-color" stylesheets="@login.css" unFocusColor="WHITE">
            <font>
                <Font name="Gill Sans MT" size="16.0" />
            </font>
         <focusColor>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#012518" />
                  <Stop color="#15959e" offset="1.0" />
               </stops>
            </LinearGradient>
         </focusColor>
        </JFXPasswordField>
        <JFXTextField fx:id="username" layoutX="126.0" layoutY="162.0" prefHeight="34.0" prefWidth="196.0" promptText="Username" styleClass="text-color" stylesheets="@login.css" unFocusColor="WHITE">
            <font>
                <Font name="Gill Sans MT" size="16.0" />
            </font>
         <focusColor>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#012518" />
                  <Stop color="#15959e" offset="1.0" />
               </stops>
            </LinearGradient>
         </focusColor>
        </JFXTextField>
        <ImageView fitHeight="79.0" fitWidth="79.0" layoutX="190.0" layoutY="33.0">
            <image>
                <Image url="@login.png" />
            </image>
        </ImageView>
      <Label fx:id="isConnected" alignment="CENTER" contentDisplay="CENTER" layoutX="28.0" layoutY="419.0" prefHeight="44.0" prefWidth="398.0" style="-fx-font-weight: bold;" textAlignment="CENTER" textFill="#65080d">
         <font>
            <Font name="Lato Regular" size="14.0" />
         </font>
      </Label>
      <JFXRadioButton fx:id="adminToggle" layoutX="126.0" layoutY="301.0" onAction="#adminToggleOnAction" styleClass="radio-Btn" stylesheets="@login.css" text="Admin" textFill="WHITE">
         <font>
            <Font name="Gill Sans MT" size="15.0" />
         </font>
      </JFXRadioButton>
      <JFXRadioButton fx:id="employeeToggle" layoutX="238.0" layoutY="301.0" onAction="#employeeToggleOnAction" styleClass="radio-Btn" stylesheets="@login.css" text="Employee" textFill="WHITE">
         <font>
            <Font name="Gill Sans MT" size="15.0" />
         </font>
      </JFXRadioButton>
    </children>
</AnchorPane>
